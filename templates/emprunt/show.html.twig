{% extends 'admin/base.html.twig' %}

{% block title %}Modifier Emprunt{% endblock %}

{% block body %}
    <h1>Modifier Emprunt</h1>
<a href="{{ path("emprunt_index") }}">
        <i class="fa fa-reply"></i>
</a>
<br>
<br>
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ emprunt.id }}</td>
            </tr>
            <tr>
                <th>Date_emprunt</th>
                <td>{{ emprunt.dateEmprunt ? emprunt.dateEmprunt|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Date_retour</th>
                <td>{{ emprunt.dateRetour ? emprunt.dateRetour|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>



    <a href="{{ path('emprunt_edit', {'id': emprunt.id}) }}">Mofidier</a>

    {{ include('emprunt/_delete_form.html.twig') }}
{% endblock %}
